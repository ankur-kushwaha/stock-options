{
  "extends": [ "plugin:react/recommended"],
    "settings": {
      "react": {
        "createClass": "createReactClass",
                                          
        "pragma": "React"  
      }
    },
    "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest/globals": true
    },
    "plugins": [
      "react",
      "jest"
    ],
    
    "rules": {
      "indent":["warn",2],
      "no-undef":"warn",
      "no-unused-vars":"warn",
      "react/no-unescaped-entities": "off",
      "@next/next/no-page-custom-font": "off",
      "react/prop-types":0,
      "react/display-name":0
    }
  }