{
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "indent":["warn",2],
      "no-undef":"warn",
      "react/no-unescaped-entities": "off",
      "@next/next/no-page-custom-font": "off"
    }
  }